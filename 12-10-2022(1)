#include<bits/stdc++.h>

using namespace std;

string isPalindrome(string S)
{
    string P = S;
 
    reverse(P.begin(), P.end());
 
    if (S == P) {
        return "Palindrome";
    }
    else {
        return "Not Palindrome";
    }
}

int main() {
    string magic;
    getline(cin,magic);
    cout<<isPalindrome(magic);
    return 0;
}
